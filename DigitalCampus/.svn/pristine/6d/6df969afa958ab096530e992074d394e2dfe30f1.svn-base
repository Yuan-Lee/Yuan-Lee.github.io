		<script>
			$(function(){

				// 选择日期
				$('.date-picker').datetimepicker({
					language: 'zh-CN',
					format: 'yyyy-mm-dd hh:ii',
					pickerPosition: 'bottom-left',
					autoclose: true,
					todayHighlight: true
				}).next().on('click', function(){
					$(this).prev().focus();
				});

				// 选择教师
				$('.js-multiSelectTeacher').click(function(){
					layer.open({
						type: 1,
						shade: .5,
						title: ['编辑任课老师','font-size:16px;'],
						area: '500px',
						btn: ['保存','取消'],
						btnAlign: 'c',
						content: $('.layer-multiSelectTeacher')
					})
				});

				// 保存
				$('.js-release').on('click',function(){
					layer.open({
						type: 1,
						shade: .5,
						title: ['提示'],
						area: '355px',
						content: $('.layer-tip')
					})
				})

				// 保存
				$('.js-save').on('click',function(){
					layer.msg('保存成功', {
						offset: 't',
						time: 2000
					});
				})

			})
		</script>
