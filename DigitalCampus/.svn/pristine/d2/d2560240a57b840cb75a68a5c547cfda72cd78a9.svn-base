		<script>
			$(function(){

				// 新建系统
				$('.js-addNewSystem').click(function(){
					layer.open({
						type: 1,
						shade: .5,
						title: ['新建选课系统','font-size:16px;'],
						area: '350px',
						btn: ['保存','取消'],
						btnAlign: 'c',
						content: $('.layer-addNewSystem')
					})
				})

				// 删除选课系统
				$('.js-delSystemFlow').click(function(){
					var that=$(this);
					layer.open({
						type: 1,
						shade: .5,
						title: ['删除','font-size:16px;'],
						area: '350px',
						btn: ['确定','取消'],
						yes: function(index){
							that.closest('.box').remove();
							layer.close(index);
						},
						content: $('.layer-delSystem')
					})
				})
			})
		</script>
