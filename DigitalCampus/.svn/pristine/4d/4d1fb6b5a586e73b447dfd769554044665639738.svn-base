		<script>
			$(function(){

				// 提交选课结果
				$('.js-submit').click(function(){
					var that=$(this);
					
					if(that.text()=="修改"){

						$(this).text('提交');
						$('.table-selectCourse input[type=checkbox]').removeAttr('disabled');
						return;
					}
					layer.open({
						type: 1,
						shade: .5,
						title: ['提示','font-size:16px;'],
						move: true,
						area: '400px',
						btn: ['提交并退出','取消重选'],
						yes: function(index){
							that.text('修改');
							$('.table-selectCourse input[type=checkbox]').attr('disabled','disabled');
							layer.close(index);
						},
						btnAlign: 'C',
						content: $('.layer-resultOfCcourseSelect')
					})

				})

				$('.table-selectCourse > tbody > tr').on('click',function(){
					var $checkbox=$(this).find('input[type=checkbox]')
					if($checkbox.is(':checked')){
						$checkbox.prop('checked',false)
					}else{						
						$checkbox.prop('checked',true);
					}
				})

				// 超出选课数量提示
				$('.js-select').click(function(){
					layer.alert('您已超过3门课程，请删除一门才可继续选择',{icon:7});
				})

			})
		</script>
