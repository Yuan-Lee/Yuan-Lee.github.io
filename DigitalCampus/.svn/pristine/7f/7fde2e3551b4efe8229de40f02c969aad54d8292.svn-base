		&lt;/select&gt;\
		&lt;select class="chosen-select" data-placeholder="任课教师"&gt;\
		&lt;option value=""&gt;&lt;/option&gt;\
		&lt;option value=""&gt;&lt;/option&gt;\
		&lt;/select&gt;\
		&lt;a class="form-group-del color-red" href="javascript:void(0);"&gt;&lt;i class="fa fa-trash"&gt;&lt;/i&gt;&lt;/a&gt;\
		&lt;/div&gt;</span><span class="nt"></span>';
	$(this).parent().find('.form-box').append($form_group_item);      //新增拼接的内容
	});
	$('.form-box-wrap').on('click','.form-group-del',function(){      //删除新增内容
	$(this).parent().remove();
	});
