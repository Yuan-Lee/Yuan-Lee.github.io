		<script>
			$(function(){

				// 批次班级展开收起
				$('.schedule-collapse-btn').on('click',function(){
					var $btnHtml_down='展开 <i class="fa fa-angle-down"></i>',
						$btnHtml_up='收起 <i class="fa fa-angle-up"></i>',
						$content=$('.schedule-collapse-content');
					
					$content.toggleClass('collapsed');
					if($content.hasClass('collapsed')){
						$(this).html($btnHtml_down)
						$content.scrollTop(0)
					}else{
						$(this).html($btnHtml_up)
					}
					
				})

				// 选择批次
				var $table=$('.table-schedule');
				$('.js-schedule-labels label').on('click',function(){
					if($(this).hasClass('selected')){
						$(this).removeClass('selected');
						$table.attr('data-filter',100);
					}else{
						$(this).addClass('selected').siblings().removeClass('selected');
						$table.attr('data-filter',$(this).attr('data-filter'));
					}
				});
				
				$('.table-schedule .edited').click(function(e){

					var currentLabel=$(this).find('label').attr("data-label"),
						dataFilter = $table.attr('data-filter');
					if (dataFilter == 100) {
						alert('请先选择需要进行的操作！');
					} else if ( currentLabel==dataFilter){
						$(this).empty();
					} else if (dataFilter == 0){
						$(this).empty();
					} else {
						var label = '<label class="schedule-label schedule-label-'+dataFilter+' thin selected" data-label="'+dataFilter+'">批次'+dataFilter+'</label>';
						$(this).empty().append(label);
					};
				});
			})
		</script>
