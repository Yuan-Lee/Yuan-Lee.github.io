		<script>
			$(function(){

				// 查看学生-编辑
				$('.show-details-btn').on('click', function(e) {
					e.preventDefault();
					$(this).closest('tr').next().toggleClass('open').siblings('.detail-row').removeClass('open');
				});
				$('.detail-row button[data-action=cancel]').on('click',function(){
					$(this).closest('tr').removeClass('open');
				})

				// 查看学生-脱离班级
				$('.js-outOfClass').click(function(){
					var that=$(this);
					layer.confirm('脱离该组合班后，该学生将以单科开班方式进行开班。确定需要脱离吗？',{
						title: ['脱离','font-size:16px;'],
						btn: ['确定','取消'],
						yes: function(index){
							that.closest('tr').next('.detail-row').remove().end().remove();
							layer.close(index);
						}
					})
				})
			})
		</script>
